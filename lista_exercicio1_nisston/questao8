# Programa que determina se um número é primo ou não

num = int(input("Digite um número:"))

if num < 2:
    print(f"{num} não é primo.")

else:
    primo = True  # assumimos que é primo

for i in range(2, num):

    if num % i == 0:
        primo = False
        break

    if num:
        print(f"{num} é primo!")

    else:
        print(f"{num} não é primo.") 